{
  "host": "localhost",
  "port": 3035,
  "paginate": {
    "default": 10,
    "max": 50
  },
  "db": {
    "folder": "nedb"
  },
  "management": {
    "path": "/account/manage",
    "from": "noreply@saucecode.tech",
    "product": "Legion CMD"
  },
  "authentication": {
    "secret": "",
    "strategies": ["jwt", "local"],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "cmd": {
    "service": {
      "interval": 30,
      "path": "_services"
    }
  }
}
